{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "semicolon": false,
      "quotemark": ["single"],
      "no-console": false,
      "trailing-comma": [false],
      "align": [false, "arguments"],
      "no-plusplus": false,
      "no-increment-decrement": false
    },
    "rulesDirectory": []
}